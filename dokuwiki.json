{
/*
	// Place your snippets for DokuWiki here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
*/
"Link": {
		"prefix": "link",
		"body": [
			"[[${1:Lien}|${2:NomLien}]]",
	
		],
		"description": "Insert a link syntax"
	},

	"Title 1": {
		"prefix": "Title",
		"body":[ "====== $1 ======",
		],
		"description": "Title 1"
	},

	"Title 2": {
		"prefix": "Title",
		"body":[ "===== $1 =====",
		],
		"description": "Title 2"
	},
	"Title 3": {
		"prefix": "Title",
		"body":[ "==== $1 ====",
		],
		"description": "Title 3"
	},
	"Title 4": {
		"prefix": "Title",
		"body":[ "=== $1 ===",
		],
		"description": "Title 4"
	},
	"Title 5": {
		"prefix": "Title",
		"body":[ "== $1 ==",
		],
		"description": "Title 5"
	},
	"Title 6": {
		"prefix": "Title",
		"body":[ "= $1 =",
		],
		"description": "Title 6"
	},

	
	"Important": {
			"prefix": "Important",
			"body": [
				"<note important>$1</note>",
			],
			"description": "Important note"
	},

	"Bold": {
		"prefix": "Bold",
		"body": [
			"**$1**",
		],
		"description": "bold text"
},

"Quote": {
	"prefix": "quote",
	"body": [
		"<blockquote>$1<cite>${2:Autor}</cite></blockquote>",
	],
	"description": "quote text"
},

"Note": {
	"prefix": "note",
	"body": [
		"<note>$1</note>",
	],
	"description": "note box"
},

"Idea": {
	"prefix": "idea",
	"body": [
		"<note tip>$1</note>",
	],
	"description": "idea text box"
},
}
